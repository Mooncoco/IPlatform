<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linearLayoutrr_left"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:clickable="true"
    android:orientation="horizontal" >

    <RelativeLayout
        android:id="@+id/menu_inner_rl_left"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >

        <ImageButton
            android:id="@+id/btn_bar_back_left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical"
            android:background="@drawable/wyx_bar_btn_back_left"
            android:gravity="center_vertical"
            android:scaleType="fitCenter" />

        <FrameLayout
            android:id="@+id/menu_inner_fl_left"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_toRightOf="@id/btn_bar_back_left"
            android:background="@drawable/wyx_bar_bg_left"
            android:gravity="center_vertical" >

            <LinearLayout
                android:id="@+id/linearLayout2_left"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:clickable="true"
                android:gravity="center_vertical"
                android:orientation="horizontal" >
            </LinearLayout>
        </FrameLayout>
    </RelativeLayout>
</LinearLayout>